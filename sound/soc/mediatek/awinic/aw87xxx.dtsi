&i2c1 {
/* AWINIC AW87XXX Smart K PA */
	aw87xxx_pa_58@58 {
		compatible = "awinic,aw87xxx_pa_58";
		reg = <0x58>;
		reset-gpio = <&pio 63 0>;
		monitor-flag = <1>;
		pa-channel = < 0 >;
		actflag-gain-array = < 0x00 0x08 0x08 0x08 0x00 >;
		gain-reg-addr = <0x05>;
		monitor-timer-val = <3000>;
		monitor-timer-count-max = <5>;
		status = "okay";
	};

	aw87xxx_pa_59@59 {
		compatible = "awinic,aw87xxx_pa_59";
		reg = <0x59>;
		reset-gpio = <&pio 1 0>;
		monitor-flag = <1>;
		actflag-gain-array = < 0x00 0x08 0x08 0x08 0x00 >;
		gain-reg-addr = <0x05>;
		pa-channel = < 1 >;
		monitor-timer-val = <3000>;
		monitor-timer-count-max = <5>;
		status = "okay";
	};
/* AWINIC AW87XXX Smart K PA End */
};
